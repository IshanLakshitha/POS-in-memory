<AnchorPane prefHeight="400.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.fx.controllers.MainPageController">
   <children>                                                                                                                                                                             
      <Button fx:id="btnManageCustomers" layoutX="51.0" layoutY="200.0" mnemonicParsing="false" onAction="#clickManageCustomers" prefHeight="75.0" prefWidth="147.0" text="Manage Customers">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="257.0" layoutY="200.0" mnemonicParsing="false" onAction="#ClickManageitems" prefHeight="75.0" prefWidth="147.0" text="Manage Items">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
